{
  "extends": "../../tsconfig.base.json",
  "compilerOptions": {
    "baseUrl": "src",
    "paths": {
      "&/*": [
        "components/dominios/*"
      ],
      "&R/*": [
        "components/dominios/rastreamento/*"
      ],
      "&A/*": [
        "components/dominios/assistencia/*"
      ],
      "&M/*": [
        "components/dominios/maxline/*"
      ],
      "@/*": [
        "components/*"
      ]
    },
    "jsx": "preserve",
    "allowJs": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "types": [
      "node",
      "jest"
    ],
    "strict": false,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "resolveJsonModule": true,
    "isolatedModules": true,
    "incremental": true
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    "**/*.js",
    "**/*.jsx",
    "next-env.d.ts"
  ],
  "exclude": [
    "node_modules"
  ]
}
